<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Work email:</strong>
    <%= user.work_email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= user.phone %>
  </p>

  <p>
    <strong>Secret question:</strong>
    <%= user.secret_question %>
  </p>

  <p>
    <strong>Secret answer:</strong>
    <%= user.secret_answer %>
  </p>

</div>
